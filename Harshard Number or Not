#include <stdio.h>

int main()
{
   int num;
   scanf("%d",&num);
   int rem,temp;
   int sum=0;
   temp=num;
   while (temp > 0)
    {
        
        rem = temp % 10;
        sum = sum+rem;
        temp = temp / 10;
    }
   if(num%sum==0)
   {
       printf("Harshard Number");
   }
   if(num%sum!=0)
   {
       printf("Not Harshard Number");
   }

    return 0;
}
